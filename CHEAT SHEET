
print("information")
print('information')
print(value)

# ORDER OF OPERATIONS Printing basic mathematics
print(1+1)
print(1-1)
print(1*1)
# ORDER OF OPERATIONS Floating value
print(1/1)
# ORDER OF OPERATIONS Integer value
print(1//1)
print(1**1)
# ORDER OF OPERATIONS Modulo
print(1%1)

# PRINTING Strings with variables
print("information, " + value + " information")
print(value + " information")
# OR
value1 = "information"
value2 = "information"
value = value1 + " " + value2
print(value)
# PRINTING F-strings with variables
print(f"The information is {value1}")

# ORDER OF OPERATIONS Basic mathematics
value1 = 1
value2 = 2
print(value1 + 1)
# OR
print(value1 + value2)
value3 = value1 + value2

# PRINTING Basic mathematics with string value and integer value
value1 = "1"
print(int(value1) + 1)
# PRINTING String and integer value
value1 = 1
value2 = 2
value3 = value1 + value2
print("The information is " + str(value3))
print("If the information is ", value3, "then", value3, "is the information.")

#
" ' Francis ' "
' '' Francis " '

value = "information"
# STR String type value
value = "10"
# INT Integer type value
value = 10

value = input("information")
# INPUT Series of input syntaxes to change the same value but with different string values
value = input("information?")
value = input("information.")
# INPUT Series of input syntaxes to change the same value but with different integer values
value = 0

# INPUT 
value1 = input("information")
value2 = int(value1)
#OR
input(int(input("information")))
