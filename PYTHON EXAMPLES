    tax_rate_1 = 5
    tax_calculation_part_1 = float(tax_input) * float(tax_rate_1) / 100
    tax_calculation_part_2 = (float(tax_input) + tax_calculation_part_1)
    print(f"The amount of tax that will be added accordingly to your spent currency is {tax_calculation_part_1}.")
    print(f"The total amount that you will pay is {tax_calculation_part_2}.")
if 1000000 <= tax_input < 5000000:
