math_result = 0
while True:
    math_equation = float(input("Please put a random number for me to multiply. If it is an even number it shall be divided by 2, if it is an odd number, by 3, type -1 to stop this program from running. "))
    if math_equation == -1:
        break
    elif math_equation % 2 == 0:
        math_result = math_equation / 2
        print(f"The number you provided is now {math_result}.")
    elif math_equation < 1000:
        math_result = math_equation / 3
        print(f"The number you provided is now {math_result}.")
    elif math_equation > 1000:
        math_result = math_equation / 30
        print(f"The number you provided is now {math_result}.")
print("Thank you for using the program!")
