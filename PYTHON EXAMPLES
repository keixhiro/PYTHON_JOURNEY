print("You want to find out the orders in the letters? Then use the program below.")
print("Please type in the letters one by one.")
letter_1 = input("Please type in a letter. ")
letter_2 = input("Please type in another letter. ")
letter_3 = input("Please type in a new letter. ")
all_letters = [letter_1, letter_2, letter_3]
all_letters_capital = all_letters.upper()
sorted_all_letters = sorted(all_letters_capital)
insig_1 = {"the first letter", "first", "the first"}
insig_2 = {"the middle letter", "middle", "the middle"}
insig_3 = {"the last letter", "last", "the last"}
first = sorted_all_letters
middle = sorted_all_letters
last = sorted_all_letters
answer_1 = input("What order would you like to know first? ")
if answer_1 in insig_1:
 print(sorted_all_letters[0])
if answer_1 in insig_2:
 print(sorted_all_letters[1])
if answer_1 in insig_3:
 print(sorted_all_letters[2])
